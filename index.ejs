<!DOCTYPE html>
<html>
<head>
	<title>SPOILER 2020</title>

	<meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
	<link rel="icon" type="image/png" href="assets/logo.png"/>

	<!-- Import lib -->
	<link rel="stylesheet" type="text/css" href="fontawesome-free/css/all.min.css">
	<link href="https://fonts.googleapis.com/css2?family=Roboto&display=swap" rel="stylesheet">
	<!-- End import lib -->

	<link rel="stylesheet" type="text/css" href="style.css">
</head>
<body class="overlay-scrollbar">

	
	<!-- navbar -->
	<div class="navbar">
		<!-- nav left -->
		<ul class="navbar-nav">
			<li class="nav-item">
				<a class="nav-link">
					<i class="fas fa-bars" onclick="collapseSidebar()"></i>
				</a>
			</li>
		</ul>
		<!-- end nav left -->
		<!-- nav right -->
		<ul class="navbar-nav nav-right">
			<li class="nav-item mode">
				<a class="nav-link" href="#" onclick="switchTheme()">
					<i class="fas fa-moon dark-icon"></i>
					<i class="fas fa-sun light-icon"></i>
				</a>
			</li>
		</ul>
		<!-- end nav right -->
	</div>
	<!-- end navbar -->

		<!-- sidebar -->
		<div class="sidebar">
			<ul class="sidebar-nav">
				<li class="sidebar-nav-item">
					<a href="/map/index.html" class="sidebar-nav-link">
						<div>
							<i class="fas fa-tachometer-alt"></i>
						</div>
						<span> Home</span>
					</a>
				</li>
				<li class="sidebar-nav-item">
					<a href="/" class="sidebar-nav-link">
						<div>
							<i class="fas fa-tachometer-alt"></i>
						</div>
						<span> Dashboard</span>
					</a>
				</li>
				<li class="sidebar-nav-item">
					<a href="/map/map.html" class="sidebar-nav-link active">
						<div>
							<i class="fab fa-accusoft"></i>
						</div>
						<span> Map </span>
					</a>
				</li>
				<li class="sidebar-nav-item">
					<a href="/map/graph.html" class="sidebar-nav-link active">
						<div>
							<i class="fab fa-accusoft"></i>
						</div>
						<span> Graph </span>
					</a>
				</li>
				<li class="sidebar-nav-item">
					<a href="/map/Accessibility statement.html" class="sidebar-nav-link active">
						<div>
							<i class="fab fa-accusoft"></i>
						</div>
						<span> Accessibility Statement </span>
					</a>
				</li>
				<li class="sidebar-nav-item">
					<a href="/map/about.html" class="sidebar-nav-link active">
						<div>
							<i class="fab fa-accusoft"></i>
						</div>
						<span> About </span>
					</a>
				</li>
				<li class="sidebar-nav-item">
					<a href="/map/contact.html" class="sidebar-nav-link active">
						<div>
							<i class="fab fa-accusoft"></i>
						</div>
						<span> Contact </span>
					</a>
				</li>

			</ul>
		</div>
		<!-- end sidebar -->

	<!-- main content -->
	<div class="wrapper">
		<div class="row">
			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-primary">
					<p>
						<i class="fas fa-tasks"></i>
					</p>
					<p>Total Cases</p>
					<h3><%= JSON.stringify(data2.confirmed) %></h3>
					
				</div>
			</div>
			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-warning">
					<p>
						<i class="fas fa-spinner"></i>
					</p>
					<p>Active Cases</p>
					<h3><%= JSON.stringify(data2.active) %></h3>	
				</div>
			</div>
			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-success">
					<p>
						<i class="fas fa-check-circle"></i>
					</p>
					<p>Discharged</p>
					<h3><%= JSON.stringify(data2.recovered) %></h3>
				</div>
			</div>
			<div class="col-3 col-m-6 col-sm-6">
				<div class="counter bg-danger">
					<p>
						<i class="fas fa-check-circle"></i>
					</p>
					<p>Deaths</p>
					<h3><%= JSON.stringify(data2.deaths) %></h3>					
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-8 col-12 col-12">
				<div class="card">
					<div class="card-header">
						<h3>
							Table
						</h3>
						<i class="fas fa-ellipsis-h"></i>
					</div>
					<div class="card-content">
						<table>
							<thead>
								<tr>
									<th>ID</th>
									<th>State</th>
									<th>Active</th>
									<th>Confirmed</th>
									<th>Recovered</th>
									<th>Deaths</th>
								</tr>
							</thead>

							<tbody>
								<% data1.forEach(function(data) { %>
								<tr>
									<td><%= data.id %></td>
									<td><%= data.state %></td>
									<td><%= data.active %></td>
									<td><%= data.confirmed %></td>
									<td><%= data.recovered %></td>
									<td><%= data.deaths %></td>
								</tr>
								<% }); %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end main content -->

	<!-- import script -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.js"></script>
	<script src="index.js"></script>
	<!-- end import script -->
</body>
</html>